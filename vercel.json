{
  "version": 2,
  "name": "brand-backend",
  "builds": [
    {
      "src": "backend/api/index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "backend/api/index.js"
    },
    {
      "src": "/uploads/(.*)",
      "dest": "backend/uploads/$1"
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}
